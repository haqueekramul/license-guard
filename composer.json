{
    "name": "developer-ekramul/license-guard",
    "description": "Laravel License Guard package to protect your software",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DeveloperEkramul\\LicenseGuard\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DeveloperEkramul\\LicenseGuard\\LicenseServiceProvider"
            ],
            "aliases": {
                "License": "DeveloperEkramul\\LicenseGuard\\Facades\\License"
            }
        }
    }
}
